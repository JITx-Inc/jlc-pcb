      "JLC04161H-7628B": [0.4 [FR4(\|7628|, 0.218, "7628*1") FR4(\|7628|, 0.218, "7628*1") FR4(\|2116|, 0.1164, "2116*1")]]
      "JLC04161H-3313A": [1.065 [FR4(\|3313|, 0.107, "3313*1") FR4(\|3313|, 0.0994, "3313*1")]]
