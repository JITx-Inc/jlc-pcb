      "JLC04121H-2116A": [0.6 [FR4(\|2116|, 0.124, "2116*1") FR4(\|2116|, 0.1164, "2116*1")]]
      ; JLC04161H-* : thickness 1.6mm
      "JLC04161H-7628": [1.065 [FR4(\|7628|, 0.2104, "7628*1")]]
